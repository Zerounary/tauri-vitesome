{
  "name": "vitesome",
  "version": "1.1.1",
  "description": "A simple opinionated Vue3 Starter Template with Vite.js",
  "scripts": {
    "dev": "vite",
    "tauri": "tauri",
    "build": "vite build",
    "test": "vitest",
    "test-one": "vitest run",
    "coverage": "vitest --coverage",
    "serve": "vite preview",
    "commit": "git-cz -a",
    "release": "standard-version -a"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test-one"
    }
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "pinia": "^2.0.13",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.34",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@iconify/iconify": "^2.2.1",
    "@iconify/json": "^2.1.24",
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@tauri-apps/cli": "^1.2.0",
    "@types/node": "^17.0.23",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vueuse/core": "^8.2.4",
    "c8": "^7.11.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "ghooks": "^2.0.4",
    "pathe": "^0.2.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.3",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vite-plugin-fonts": "^0.3.0",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-purge-icons": "^0.8.1",
    "vite-plugin-windicss": "^1.8.3",
    "vite-svg-loader": "^3.2.0",
    "vitest": "^0.10.0",
    "vue-tsc": "^0.33.9",
    "windicss": "^3.5.1"
  }
}
